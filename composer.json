{
    "name": "ctessier/nova-advanced-image-field",
    "description": "An advanced image field for Nova with crop and resize.",
    "license": "MIT",
    "homepage": "https://github.com/ctessier/nova-advanced-image-field",
    "keywords": [
        "laravel",
        "nova",
        "image",
        "crop",
        "resize",
        "image crop",
        "image resize",
        "advanced",
        "advanced image",
        "advanced image field"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "authors": [
        {
            "name": "ClÃ©ment Tessier",
            "email": "clement.tessier@ymail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "^3.0",
        "intervention/image": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Ctessier\\NovaAdvancedImageField\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ctessier\\NovaAdvancedImageField\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ctessier\\NovaAdvancedImageField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
